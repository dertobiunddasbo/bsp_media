# Supabase
NEXT_PUBLIC_SUPABASE_URL=
NEXT_PUBLIC_SUPABASE_ANON_KEY=
SUPABASE_SERVICE_ROLE_KEY=

# E-Mail (Resend)
RESEND_API_KEY=

# TinyMCE
NEXT_PUBLIC_TINYMCE_API_KEY=

# Optionale Domain für Metadaten/OG (z. B. https://www.bsp-media.de)
# NEXT_PUBLIC_SITE_URL=https://www.bsp-media.de

# Google Analytics 4 – Measurement ID (z. B. G-XXXXXXXXXX)
# Nur wenn gesetzt: GA4 mit Consent Mode wird eingebunden; sonst keine Analytics-Skripte.
# NEXT_PUBLIC_GA_MEASUREMENT_ID=G-XXXXXXXXXX

# Google Search Console – Verifikationscode aus der Search Console
# Setzen, um die Domain in der Search Console zu verifizieren (meta-Tag wird ins Layout eingefügt).
# NEXT_PUBLIC_GOOGLE_SITE_VERIFICATION=xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
